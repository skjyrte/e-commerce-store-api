{
  "name": "e-commerce-store-api",
  "version": "1.0.0",
  "description": "API of e-commence store",
  "main": "dist/src/server.js",
  "engines": {
    "node": "20.15.1"
  },
  "scripts": {
    "nodemondev": "cross-env NODE_ENV=development NODE_OPTIONS='--trace-warnings' nodemon -L --exec \"node --loader ts-node/esm\" ./src/server.ts",
    "tsndev": "cross-env NODE_ENV=development node --loader ts-node/esm --experimental-specifier-resolution=node ./src/server.ts",
    "build": "tsc",
    "prod": "npm run build && cross-env NODE_ENV=production node ./dist/server.js",
    "test": "jest --silent=false",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.2",
    "@types/pg": "^8.11.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "globals": "^15.8.0",
    "pg": "^8.12.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/format": "^19.3.0",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "commitizen": "^4.3.0",
    "commitlint": "^19.3.0",
    "conventional-changelog-atom": "^5.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-jest": "^28.6.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.2",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.17.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "eslint --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
